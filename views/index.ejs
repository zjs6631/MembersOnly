<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% if (errors) { %>
      <ul>
        <% errors.forEach(function(error){ %>
          <li> <%= error.msg %></li>
        <% }) %>
      </ul>
    <% } %>
  </body>
</html>
